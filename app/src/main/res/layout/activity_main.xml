<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="111dp"
        android:layout_marginBottom="106dp"
        android:text="Locked Down"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/sensor_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <Button
        android:id="@+id/User_Management"
        android:layout_width="240dp"
        android:layout_height="72dp"
        android:layout_marginBottom="248dp"
        android:text="@string/User"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sensor_button"
        app:layout_constraintVertical_bias="1.0" />


    <Button
        android:id="@+id/sensor_button"
        android:layout_width="240dp"
        android:layout_height="72dp"
        android:layout_marginBottom="401dp"
        android:text="Sensor"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="185dp"
        android:layout_height="0dp"
        android:layout_marginTop="163dp"
        android:layout_marginBottom="35dp"
        android:text="Select A Paqe"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/sensor_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>
